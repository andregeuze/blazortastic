@page "/Testobjectje"
@using Blazortastic.Data
@using GeneratorHelper

<PageTitle>Testobjectje</PageTitle>
<MudText Typo="Typo.h3" GutterBottom="true">Testobjectje</MudText>
@if(TestobjectjeList == null)
{
    <MudProgressCircular Color = "Color.Default" Indeterminate = "true" />
}
else
{
    <MudTable Items="TestobjectjeList" Hover="true" Elevation="0">
        <HeaderContent>
            <MudTh>Name</MudTh>
        </HeaderContent>
        <RowTemplate>
            <MudTd DataLabel="Name">@context.Name</MudTd>
        </RowTemplate>
    </MudTable>
}